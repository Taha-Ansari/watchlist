{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": 0,
        "no-plusplus": "off",
        "no-param-reassign": "off",
        "prefer-const": "off",
        "no-unused-vars": "warn",
        "no-shadow": "off",
        "no-var": "off",
        "vars-on-top": "off",
        "radix":"off",
        "quotes": [
            "error",
            "double"
          ]

    }
}
